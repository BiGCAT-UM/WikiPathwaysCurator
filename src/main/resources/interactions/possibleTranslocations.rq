SELECT DISTINCT ?homepage ?source ?sourceLabel ?target ?targetLabel
WHERE {
   ?pathway a wp:Pathway ;
            foaf:page ?homepage .
   ?interaction dcterms:isPartOf ?pathway ; 
      a wp:Interaction ;
      wp:target ?target ;
      wp:source ?source .
   ?source a wp:DataNode ;
      rdfs:label ?sourceLabel ;
      wp:bdbWikidata ?sourceQ .
   ?target a wp:DataNode ;
      rdfs:label ?targetLabel ;
      wp:bdbWikidata ?dataQ .
   FILTER (?sourceQ = ?targetQ)
}
